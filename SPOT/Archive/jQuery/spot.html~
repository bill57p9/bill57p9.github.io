<!DOCTYPE html>
<html>
	<body>
		<script language="javascript" src="http://www.nearby.org.uk/tests/geotools2.js"> </script>
		<script language="javascript" src="jquery.min.js"> </script>
		<script language="javascript">
			$.getJSON("https://arvell.dyndns.org/spot/latest.json?callback=?", function(data)
			{
				console.log(data);
				$.each(data.entries, function(i, f)
				{
					var tblRow="<tr><td>"+f.messengerName+"</td><td>"+f.unixTime+"</td><td></td><td></td></tr>";
					$(tblRow).appendTo("#locationData tbody");
				});
			});
		</script>
		<table id="locationData">
			<tr>
				<th>Tracker</th>
				<th>Timestamp</th>
				<th>Location</th>
				<th>Message</th>
			</tr>

	
		</table>
	</body>
</html>

